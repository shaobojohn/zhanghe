<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ubuntu.example">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CollapsingToolbarActivity"
            android:theme="@style/AppTheme.NoActionBar"/>

        <activity android:name=".CustomTextActivity"
            android:theme="@style/AppTheme.NoActionBar"/>

        <activity android:name=".NestedTestActivity"
            android:theme="@style/AppTheme.NoActionBar"
           />
        <service android:name="com.example.aidl.TestAidlService"/>
    </application>

</manifest>